<h1>My Profile</h1>
<br>
<div>
  {{# if message}}
    <div class="alert alert-success">
        {{message}}
    </div>
  {{/if}}
  <p class="h5">Name: {{name}}</p>
  <p class="h5">Email: {{email}}</p>
  <p class="h5">Phone Number: {{phoneNumber}}</p>
  <br>
  <a href="/account/edit"><button class="btn btn-primary">Edit Profile</button></a>
  <a href="/account/changePassword"><button class="btn btn-primary">Change password</button></a>
</div>
